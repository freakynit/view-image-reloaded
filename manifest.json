{
  "manifest_version": 2,

  "name": "View Image Reloaded",
  "description": "Google Images's View Image Button Reloaded",
  "version": "1.1",

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },

  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"]
    }
  ],


  "permissions": [
    "tabs"
  ]
}
